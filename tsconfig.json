{
  "compilerOptions": {
    "target": "ESNext",
    "module": "es6",
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  },
  "ts-node": {
      "esm": true
  },
  "lib": ["esnext"],
  "exclude": [
      "../../node_modules",
      "../../typings"
  ]
}